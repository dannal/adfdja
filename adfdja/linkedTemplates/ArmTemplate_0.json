{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfdja"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"CosmosDbNoSql1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'CosmosDbNoSql1'"
		},
		"adsstoragedatalake_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adsstoragedatalake'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://testdatalakestgdja.dfs.core.windows.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/default')]",
			"type": "Microsoft.DataFactory/factories/managedVirtualNetworks",
			"apiVersion": "2018-06-01",
			"properties": {},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AutoResolveIntegrationRuntime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				},
				"managedVirtualNetwork": {
					"type": "ManagedVirtualNetworkReference",
					"referenceName": "default"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/managedVirtualNetworks/default')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/AzureDataLakeStorage1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					}
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CosmosDbNoSql1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "CosmosDb",
				"typeProperties": {
					"connectionString": "[parameters('CosmosDbNoSql1_connectionString')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/adsstoragedatalake')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureBlobStorage",
				"typeProperties": {
					"connectionString": "[parameters('adsstoragedatalake_connectionString')]"
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DestinationDataset_7y3')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "adsstoragedatalake",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Parquet",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"folderPath": "Daily/parquet",
						"container": "staging"
					},
					"compressionCodec": "snappy"
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/adsstoragedatalake')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/SourceDataset_7y3')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "adsstoragedatalake",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "Json",
				"typeProperties": {
					"location": {
						"type": "AzureBlobStorageLocation",
						"folderPath": "Daily",
						"container": "globaldata"
					}
				},
				"schema": {
					"type": "object",
					"properties": {
						"date": {
							"type": "string"
						},
						"total_cases": {
							"type": "integer"
						},
						"new_cases": {
							"type": "integer"
						},
						"total_deaths": {
							"type": "integer"
						},
						"new_deaths": {
							"type": "integer"
						},
						"total_cases_per_million": {
							"type": "number"
						},
						"new_cases_per_million": {
							"type": "number"
						},
						"total_deaths_per_million": {
							"type": "integer"
						},
						"new_deaths_per_million": {
							"type": "integer"
						},
						"stringency_index": {
							"type": "number"
						},
						"new_cases_smoothed": {
							"type": "number"
						},
						"new_deaths_smoothed": {
							"type": "integer"
						},
						"new_cases_smoothed_per_million": {
							"type": "number"
						},
						"new_deaths_smoothed_per_million": {
							"type": "integer"
						},
						"Code": {
							"type": "string"
						},
						"Country": {
							"type": "string"
						}
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/adsstoragedatalake')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/CopyPipeline_7y3')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy_7y3",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [
							{
								"name": "Source",
								"value": "globaldata/Daily/"
							},
							{
								"name": "Destination",
								"value": "staging/Daily/parquet/"
							}
						],
						"typeProperties": {
							"source": {
								"type": "JsonSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"wildcardFileName": "*",
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "JsonReadSettings"
								}
							},
							"sink": {
								"type": "ParquetSink",
								"storeSettings": {
									"type": "AzureBlobStorageWriteSettings"
								},
								"formatSettings": {
									"type": "ParquetWriteSettings"
								}
							},
							"enableStaging": false,
							"validateDataConsistency": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"path": "$['date']"
										},
										"sink": {
											"name": "date"
										}
									},
									{
										"source": {
											"path": "$['total_cases']"
										},
										"sink": {
											"name": "total_cases"
										}
									},
									{
										"source": {
											"path": "$['new_cases']"
										},
										"sink": {
											"name": "new_cases"
										}
									},
									{
										"source": {
											"path": "$['total_deaths']"
										},
										"sink": {
											"name": "total_deaths"
										}
									},
									{
										"source": {
											"path": "$['new_deaths']"
										},
										"sink": {
											"name": "new_deaths"
										}
									},
									{
										"source": {
											"path": "$['total_cases_per_million']"
										},
										"sink": {
											"name": "total_cases_per_million"
										}
									},
									{
										"source": {
											"path": "$['new_cases_per_million']"
										},
										"sink": {
											"name": "new_cases_per_million"
										}
									},
									{
										"source": {
											"path": "$['total_deaths_per_million']"
										},
										"sink": {
											"name": "total_deaths_per_million"
										}
									},
									{
										"source": {
											"path": "$['new_deaths_per_million']"
										},
										"sink": {
											"name": "new_deaths_per_million"
										}
									},
									{
										"source": {
											"path": "$['stringency_index']"
										},
										"sink": {
											"name": "stringency_index"
										}
									},
									{
										"source": {
											"path": "$['new_cases_smoothed']"
										},
										"sink": {
											"name": "new_cases_smoothed"
										}
									},
									{
										"source": {
											"path": "$['new_deaths_smoothed']"
										},
										"sink": {
											"name": "new_deaths_smoothed"
										}
									},
									{
										"source": {
											"path": "$['new_cases_smoothed_per_million']"
										},
										"sink": {
											"name": "new_cases_smoothed_per_million"
										}
									},
									{
										"source": {
											"path": "$['new_deaths_smoothed_per_million']"
										},
										"sink": {
											"name": "new_deaths_smoothed_per_million"
										}
									},
									{
										"source": {
											"path": "$['Code']"
										},
										"sink": {
											"name": "Code"
										}
									},
									{
										"source": {
											"path": "$['Country']"
										},
										"sink": {
											"name": "Country"
										}
									}
								]
							}
						},
						"inputs": [
							{
								"referenceName": "SourceDataset_7y3",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "DestinationDataset_7y3",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/SourceDataset_7y3')]",
				"[concat(variables('factoryId'), '/datasets/DestinationDataset_7y3')]"
			]
		}
	]
}